{% extends "templates/base.twig" %}

{% block title %}Post view {% endblock %}

{% block body %}



    
    <h1> Books Available In Library </h1>
    
    <table id="books">
        <thead>
            <tr align="center">

                <th>
                    Book Name
                </th>
                
                <th>
                    Number of books
                </th>
             
            </tr>
        </thead>

        <tbody>

            {% for bookdata in booksavailable %}
                <tr align="center">

                    <td>
                        {{ bookdata.bookname }}
                    </td>
                     <td>
                        {{ bookdata.number }}
                    </td>
                    
                {% endfor %}

            </tbody>

        </table>
    </html>
{% endblock %}
